#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d", &n);
    int arr[101];    // cuz 0-99
   for(int i=0; i<n; i++){
       scanf("%d", &arr[i]);
   }
    int x, p;
    scanf("%d%d", &x, &p);
    for(int i=n-1; i>=p; i--){
        arr[i+1] = arr[i];
    }
    arr[p]=x;
    for(int i=0; i<=n; i++){
        printf("%d ", arr[i]);
    }
    return 0;
